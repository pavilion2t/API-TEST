{
  "docset" : {
    "name": "Bindo API Client",
    "enableJavascript": true
  },
  "tags": {
    "allowUnknownTags": false
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "src/",
      "README.md"
    ],
    "exclude": [],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
          "outputSourceFiles": true
      }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true
  }
}